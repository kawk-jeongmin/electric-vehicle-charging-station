<mapper namespace="com.spring.mapper.SearchMapper">
	
	<select id="listSearch" parameterType="String" resultType="SearchVO">
	SELECT * FROM location WHERE address LIKE CONCAT('%', #{keyword}, '%')
	</select>
	
</mapper>